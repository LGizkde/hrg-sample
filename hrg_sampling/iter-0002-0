                X4 -> ([X, 0] to win) | (n0.*0 :[X5, 0]@ n1.*1 (n2. :w/win-01) n3.*2 n4.*3)
                    X5 -> (Boeing Corporation) | (.*0 :ARG1 (.*2 :ARG0 (. :c/company :name (. :n/name :op1 (. :Boeing) :op2 (. :Corporation))) :ARG1 (.*3 :mod .*4 :mod .*1)))
                X1 -> (this promise is to allow) | (n0. :purpose .*0 :a/allow-01 :mod (. :t/this) :p/promise)

